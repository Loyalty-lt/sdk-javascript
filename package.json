{
  "name": "@loyaltylt/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript SDK for Loyalty.lt platform",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.esm.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "loyalty",
    "sdk",
    "javascript",
    "typescript",
    "api",
    "client",
    "websocket"
  ],
  "author": "Loyalty.lt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/loyalty-lt/sdk-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/loyalty-lt/sdk-javascript/issues"
  },
  "homepage": "https://github.com/loyalty-lt/sdk-javascript#readme",
  "dependencies": {
    "ably": "^2.10.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.50.0",
    "jest": "^29.5.0",
    "qrcode.react": "^4.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "qrcode.react": ">=3.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0",
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "qrcode.react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
